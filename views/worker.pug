div(id=id)
    p="email: "+email
    button(class="createBtn block" hx-target="this" hx-swap="afterend" hx-trigger="click" hx-get="editWorker/"+email) edit Worker
    button(class="createBtn block" hx-target="#"+id hx-swap="delete" hx-trigger="click" hx-delete="worker/"+email hx-confirm="Do you want to delete?") delete Worker